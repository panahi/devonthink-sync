{
    "private": true,
    "name": "devonthink-sync",
    "version": "0.0.1",
    "description": "A utility to synchronize Raindrop.io, Devonthink 3 and Obsidian",
    "license": "MIT",
    "author": "epanahi",
    "scripts": {
        "start": "node build/devonthink-sync.js",
        "build": "cross-env NODE_ENV=production tsc -p .",
        "clean": "rimraf build/",
        "test": "mocha \"test/**/*.ts\"",
        "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
        "watch": "tsc -p . --watch"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": false,
        "tabWidth": 4
    },
    "dependencies": {
        "@jxa/global-type": "^1.3.5",
        "@jxa/run": "^1.3.5",
        "bunyan": "^1.8.15",
        "commander": "^9.4.0",
        "dotenv": "^16.0.1",
        "querystringify": "^2.2.0"
    },
    "devDependencies": {
        "@types/bunyan": "^1.8.8",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/querystringify": "^2.0.0",
        "cross-env": "^6.0.3",
        "mocha": "^6.2.2",
        "prettier": "^1.18.2",
        "rimraf": "^3.0.0",
        "ts-node": "^8.4.1",
        "ts-node-test-register": "^8.0.1",
        "typescript": "^3.6.4"
    }
}